@startuml
!include style.puml
skinparam ClassFontColor #000000
skinparam ClassBorderColor #000000

title New state

package State {
    class State1 as "__ta0:TAssist__"
    class Student1 as "__s1:Student__"
    class Student2 as "__s2:Student__"
    class ClassGroup1 as "__c1:ClassGroup__"
    class ClassGroup2 as "__c2:ClassGroup__"
    class Module1 as "__m1:Module__"
    class Module2 as "__m2:Module__"
}

State1 *--> Student1
State1 *--> Student2
State1 *--> ClassGroup1
State1 *--> ClassGroup2
Module1 *--> ClassGroup1
Module2 *--> ClassGroup2
State1 *--> Module1
State1 *--> Module2
Student1 --> Module1
Student2 --> Module2
Student1 --> ClassGroup1
Student2 --> ClassGroup2

hide Student1
hide ClassGroup1
hide Module1

@end